{
    "name": "ChromeKeyboardShortcuts",
    "version": "0.1",
    "description": "",
    "keyboard_shortcuts": [
        { "keyCombo": ",", "fnName": "moveTabLeft" },
        { "keyCombo": ".", "fnName": "moveTabRight" },
        { "keyCombo": "right", "fnName": "moveTabsToRightSide" },
        { "keyCombo": "left", "fnName": "moveTabsToLeftSide" },
        { "keyCombo": "e", "fnName": "closeTabsToTheRight" },
        { "keyCombo": "q", "fnName": "closeTabsToTheLeft" },
        { "keyCombo": "p", "fnName": "moveTabsToNewWindow" },
        { "keyCombo": "=", "fnName": "duplicateTabs" },
        {
            "keyCombo": "8",
            "fnName": "reloadExtension",
            "args": ["iongmaeonendlpdceflfonepclmfgcee"]
        }
    ],
    "permissions": ["system.display", "tabs", "<all_urls>"],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google.com; object-src self'",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.google.com/search?*"],
            "exclude_matches": [],
            "js": ["js/contentScripts/google.bundle.js"]
        }
    ],
    "background": {
        "scripts": ["js/background.bundle.js"],
        "persistent": true
    }
}
