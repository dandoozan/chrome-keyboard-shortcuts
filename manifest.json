{
    "name": "KeyboardShortcuts",
    "version": "0.1",
    "description": "",
    "permissions": [
        "tabs",
        "*://*/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google.com; object-src 'self'",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://www.google.com/search?*"],
            "exclude_matches": [],
            "js": [
                "js/vendor/jquery-3.1.1.min.js",
                "js/vendor/mousetrap.min.js",
                "js/contentScripts/google.js"
            ]
        }
    ],
    "background": {
        "scripts": [ "js/background/background_listenForReload.js" ],
        "persistent": false
    }
}
